#!/usr/bin/env bash
#Redirect

sudo apt-get -y update
sudo apt-get -y install nginx
sudo sh -c "echo 'Holberton School' > /var/www/html/index.html"
sed -i '/listen 80 default_server/a rewrite ^/redirect_me https://fb.com/edw10/ permanent;' /etc/nginx/sites-available/default
sudo service nginx start
